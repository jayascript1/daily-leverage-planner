{
  "rewrites": [
    {
      "source": "/mcp/(.*)",
      "destination": "/api/mcp?path=/$1"
    },
    {
      "source": "/mcp",
      "destination": "/api/mcp"
    },
    {
      "source": "/.well-known/openai-apps-challenge",
      "destination": "/api/well-known-openai-apps-challenge"
    }
  ]
}
